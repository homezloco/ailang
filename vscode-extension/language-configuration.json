{
    "comments": {
        "lineComment": "#"
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        {"open": "{", "close": "}"},
        {"open": "[", "close": "]"},
        {"open": "(", "close": ")"},
        {"open": "\"", "close": "\""},
        {"open": "'", "close": "'"}
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"],
        ["<", ">"]
    ],
    "indentationRules": {
        "increaseIndentPattern": "^\\s*[a-zA-Z_][a-zA-Z0-9_]*\\s*:\\s*$",
        "decreaseIndentPattern": "^\\s*\\}\\s*$"
    },
    "wordPattern": "[a-zA-Z_][a-zA-Z0-9_]*"
}
